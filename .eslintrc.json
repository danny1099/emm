{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "space-before-function-paren":"off",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "multiline-ternary": "off"
    },
    "plugins": [
        "react"
    ]
}
